<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <command
            name="Transform to X-CProvML"
            categoryId="CProvML.ee.commands.category"
            id="CProvML.ee.commands.sampleCommand">
      </command>
      <command
            name="Transform to EC2 Calls"
            categoryId="CProvML.ee.commands.category"
            id="CProvML.ee.commands.sampleCommandTwo">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="CProvML.ee.commands.sampleCommand"
            class="cprovml.ee.xcprovmlfilter.MenuHandler">
      </handler>
      <handler
            commandId="CProvML.ee.commands.sampleCommandTwo"
            class="cprovml.ee.cloudservicecallsfilter.MenuHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="CProvML.ee.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="CProvML"
               mnemonic="M"
               id="CProvML.ee.menus.sampleMenu">
            <command
                  commandId="CProvML.ee.commands.sampleCommand"
                  mnemonic="S"
                  id="CProvML.ee.menus.sampleCommand">
            </command>
            <command
                  commandId="CProvML.ee.commands.sampleCommandTwo"
                  mnemonic="S"
                  id="CProvML.ee.menus.sampleCommandTwo">
            </command>
            <visibleWhen
                  checkEnabled="true">
               <with
                     variable="activeEditorId">
                  <or>
                     <equals
                           value="CProvML.presentation.CProvMLEditorID">
                     </equals>
                     <equals
                           value="CProvML.diagram.part.CProvMLDiagramEditorID">
                     </equals>
                  </or>
               </with>
            </visibleWhen>
         </menu>
      </menuContribution>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.e4.ui.workbench.swt.E4Application"
            name="CPS">
         <property
               name="appName"
               value="CPS">
         </property>
      </product>
   </extension>
   <extension
         id="cpsProduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="CPS">
         <property
               name="windowImages"
               value="eclipse16.png,eclipse32.png,eclipse48.png">
         </property>
         <property
               name="aboutText"
               value="%productBlurb">
         </property>
         <property
               name="aboutImage"
               value="eclipse_lg.png">
         </property>
         <property
               name="startupForegroundColor"
               value="9c9696">
         </property>
         <property
               name="startupProgressRect"
               value="2,290,448,10">
         </property>
         <property
               name="startupMessageRect"
               value="7,265,320,20">
         </property>
         <property
               name="appName"
               value="CPS">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>

</plugin>
